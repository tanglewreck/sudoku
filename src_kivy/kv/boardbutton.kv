<BoardButton>:
    canvas.before:
        Color:
            rgba: 255/255, 255/255, 255/255, 0/100
        Rectangle:
            pos: self.pos
            size: self.size
    padding: 0
    background_color: 0/255, 220/255, 0/255, 0/100
    font_size: '24sp'
    bold: True
    on_release: root.update_block_button(self.text)
